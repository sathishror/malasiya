<%= form_for(@store) do |f| %>
  <div class="form-sec-row">
    <%= label_tag :agency_id, raw("Agency <span style=color:red>*</span>"), :class=>"text" %>
    <span class="in-box">
      <%= select("ict","agency_id",  Agency.active.collect {|agency| [agency.name, agency.id]},:prompt=>"SELECT AN AGENCY") %>
    </span>
  </div>
  <div class="form-sec-row">
    <label class="text">Select Sub Category<span style=color:red>*</span></label>
    <span class="in-box">
      <%=select("ict_agency","sub_category_id", options_from_collection_for_select(SubCategory.where("category_id = 8").active.all, 'id','name'),{:prompt=>"SELECT A SUB CATEGORY"})%>
    </span>
  </div>
  <%= hidden_field_tag :resource_type, ("ict_agency") %>
  <div class="form-sec-row">
    <label class="text">Resource<span style=color:red>*</span></label>
    <span class="in-box">
      <%= select("ict_agency","resource_id", Resource.where('name is not null').active.collect {|resource| [resource.name, resource.id]},:prompt=>"SELECT A RESOURCE") %>
    </span>
  </div>
  <%#*<div class="dynamic-form">%>
  <%#*<div class="form-sec-row"><a href="#" id="add">Add</a> | <a href="#" id="remove">Remove</a> | <a href="#" id="reset">Reset</a></div>%>
  <%#*<div class="inputs"><div class="form-sec-row"> <label class="text">Serial No</label></div>%>
  <%#*<div class="form-sec-row-center">%>
  <%#*<input type="text" name=text1 size ="29" class="field"/>%>
  <%#*</div>%>
  <%#*</div>%>
  <%#*</div>%>
  <div class="form-sec-row">
    <label class="text">Serial No <span style=color:red>*</span></label>
    <span class="in-box">
      <%= f.text_field :serial_no%>
    </span>
  </div>
  <div class="form-sec-row">
    <label class="text">UOM </label>
    <span class="in-box">
      <%= f.text_field :uom%>
    </span>
  </div>
  <div class="form-sec-row">
    <label class="text">&nbsp;</label>
    <span class="in-box">
      <%= f.submit :class=>"btn2", :id=>"ict_store_submit", :value=>@store.new_record? ? "Create" : "Update"%>
    </span>
  </div>
<%end%>
