<div class="CenterNav">
  <h2 class="b-head">Manage Resource  Requisition</h2>
  <div class="create-user">
    <div class="cu-c1"><a href="/ict_network_points/list_to_select_ict" title="Create Requisition for ICT "><img src="../images/add_user.jpg" width="17" height="16" alt="img" /> Create Requisition for ICT</a></div>
  </div>
  <div class="form-sec">

    <div class="form-sec-row form-sec-row1">
      <div id="show_text"><font color='#329C20' size="2"><b><%= "Please Select the drop box to list out the paricular Requisition" %></b></font></div>
      <br/><br/>
      <div class="form-sec-row">
        <%= label "requisition","type_id",raw("Requisition Type<span style=color:red>*</span>"), :class=>"text"%>
        <span class="in-box">
          <%= select("requisition","type_id", RequisitionType.all.collect {|p| [ p.name, p.id ] },:prompt => "SELECT A ICT FACILITY", :style=> "width:132%;") %>
        </span>
      </div>

      <div id="network_for_ict" style="display:none;">
        <%=render :partial=>'ict_network_points/list'%>
      </div>

      <div id="firewall_for_ict" style="display:none;">
        <%=render :partial=>'ict_firewalls/list'%>
      </div>

      <div id="vpn_for_ict" style="display:none;">
        <%=render :partial=>'ict_vpns/list'%>
      </div>

      <%#*<div id="hardware_for_ict" style="display:none;">%>
        <%#=render :partial=>'agency_stores/agency_room'%>
      <%#*</div>%>

      <div id="software_for_ict" style="display:none;">
        <%=render :partial=>'software_installations/list'%>
      </div>

      <div id="system_access_for_ict" style="display:none;">
        <%=render :partial=>'ict_system_accesses/list'%>
      </div>





    </div></div></div>